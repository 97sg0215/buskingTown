<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="47.1dp"
    android:background="#fff"
    tools:context=".Channel_busker_fragment2">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/roadConcert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="거리공연"
            android:layout_marginLeft="46.5dp"
            android:layout_marginTop="15.2dp"
            android:textColor="@color/mainYellow"
            android:textSize="15sp"
            android:lineSpacingExtra="2sp" />

        <TextView
            android:id="@+id/practiceRoom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="연습실"
            android:textColor="@color/fontBlack"
            android:textSize="15sp"
            android:layout_marginTop="15.2dp"
            android:layout_marginLeft="62dp"
            android:lineSpacingExtra="2sp" />

        <TextView
            android:id="@+id/contacter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="콘서트장"
            android:textColor="@color/fontBlack"
            android:textSize="15sp"
            android:layout_marginTop="15.2dp"
            android:layout_marginLeft="62dp"
            android:lineSpacingExtra="2sp" />

    </LinearLayout>

    <View
        android:id="@+id/fragmentView"
        android:layout_width="60dp"
        android:layout_height="2.5dp"
        android:layout_marginTop="44.7dp"
        android:layout_marginLeft="43.5dp"
        android:background="@color/mainYellow" />

</FrameLayout>